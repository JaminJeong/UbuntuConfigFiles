
# some more ls aliases
alias f='nautilus ./ &'
alias g='git'
alias p='pwd'
alias h='history'
alias lr='ls -R'
alias '..'='cd ../'
alias '.1'='cd ../'
alias '.2'='cd ../../'
alias '.3'='cd ../../../'
alias '.4'='cd ../../../../'
alias '.5'='cd ../../../../../'

alias pycharm='$HOME/Util/pycharm-community-2019.1.2/bin/pycharm.sh'
